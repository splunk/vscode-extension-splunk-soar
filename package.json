			},
			{
				"command": "splunkSoar.assets.inspect",
				"title": "Inspect",
				"icon": "$(eye)",
				"category": "SOAR Assets"
			},
			{
				"command": "splunkSoar.containers.inspect",
				"title": "Inspect",
				"icon": "$(eye)",
				"category": "SOAR Containers"
			},
			{
				"command": "splunkSoar.containers.delete",
				"title": "Delete",
				"icon": "$(trash)",
